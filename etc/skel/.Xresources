! font
xterm*faceName: xft:Inconsolata:size=12:antialias=true 
!xterm*font:     -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
!xterm*wideFont: -misc-fixed-medium-r-normal-ja-18-120-100-100-c-180-iso10646-1
! ------------------------------------------------------------------------------
! Font configuration
! ------------------------------------------------------------------------------

!URxvt*font:				xft:M+ Ubuntu:regular:size=12
!URxvt*boldFont:			xft:M+ Ubuntu:bold:size=12
!URxvt*italicFont:		xft:M+ Ubuntu:italic:size=12
!URxvt*boldItalicFont:	xft:M+ Ubuntu:bold italic:size= 12

! ------------------------------------------------------------------------------
! Xft Font Configuration
! ------------------------------------------------------------------------------

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb


! ROFI SETUP HERE !
rofi.color-enabled: true
!               			   bg     		  border   separator
rofi.color-window: #131822, #233f66, #233f66

!                              bg      	 fg       			bg-alt   	hl-bg  		  hl-fg
rofi.color-normal: #131822, #aab0bc, #131822, #131822, #233f66
rofi.color-active:   #131822, #aab0bc, #131822, #131822, #233f66
rofi.color-urgent:   #E32608, #233f66, #E32608, #E32608, #aab0bc

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 9
rofi.font: monofur 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.line-margin: 10

